apiVersion: apps/v1
kind: Deployment
metadata:
  name: 
spec:
  replicas: 
  selector:
    matchLabels:
      app: 
  template:
    metadata:
      labels:
        app: 
    spec:
      containers:
      - name: 
        image: 
        ports:
        - containerPort: 80
---

apiVersion: v1
kind: Service
metadata:
  name: 
spec:
  selector:
    app: 
  ports:
   - protocol: TCP
   - port: 80
     targetPort: 80
  type: ClusterIP
